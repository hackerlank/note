create table oa_domain(
    id int unsigned not null auto_increment,

    title varchar(32) not null default '' comment '域名',
    type tinyint unsigned not null default 0 comment '域名类型 0 综合 1 快速充值 2 大转盘 3 自助客服 4 验证中心 5 线路检测',
    https tinyint unsigned not null default 0 comment '是否为 https 0 否 1 是',
    description varchar(256) not null default '' comment '描述',

    create_id int unsigned not null default 0 comment '创建者id  除了管理员只有自己可以删除自己域名',
    agent_id int unsigned not null default 1 comment '服务商id  1 海洋  2 金沙', 

    status TINYINT unsigned not null default 1 comment '状态 0关闭 1开启   域名关闭后前台将拿不到数据',
    
    order_id int not null default 0 comment '排序',
    create_time int unsigned not null default 0,
    update_time int unsigned not null default 0,


    key title (title),
    key type (type),
    key order_id (order_id),
    PRIMARY key (id)
)ENGINE=INNODB  DEFAULT CHARSET=utf8


// 全局参数
create table global_config(
    id int unsigned not null auto_increment,

    config_name varchar(32) not null default '' comment '参数名',
    config_type tinyint unsigned not null default 0 comment '数据类型 0 字符串 1 数值 2 对象',
    config_default_value text comment '全局配置默认参数',
    description text comment '描述',

    web_type_id tinyint unsigned not null default 0 comment '所属网站类型',

    need_auth tinyint unsigned not null default 0 comment '验证登录  0 不验证 1 验证',

    status TINYINT not null default 0 comment '状态 0关闭 1开启',
    order_id int not null default 0 comment '排序',
    create_time int unsigned not null default 0,
    update_time int unsigned null default 0,

    key config_name (config_name),
    key web_type_id (web_type_id),
    key order_id (order_id),
    PRIMARY key (id)
)ENGINE=INNODB  DEFAULT CHARSET=utf8 comment='全局配置'


// 全局 参数数据
create table global_config_value(
    id int unsigned not null auto_increment,

    config_id int unsigned not null default 0 comment '配置id',
    config_value text comment '参数的值',
    domain_id int unsigned not null default 0 comment '域名id',
    create_id int unsigned not null default 0 comment '创建者',

    status TINYINT not null default 0 comment '状态 0关闭 1开启',
    order_id int not null default 0 comment '排序',
    create_time int unsigned not null default 0,
    update_time int unsigned null default 0,

    key config_id (config_id),
    key domain_id (domain_id),
    key create_id (create_id),
    key order_id (order_id),
    PRIMARY key (id)
)ENGINE=INNODB  DEFAULT CHARSET=utf8 comment='参数数据'









create table member(
    id int unsigned not null auto_increment,


    status TINYINT unsigned not null default 0 comment '状态 0关闭 1开启',
    order_id int not null default 0 comment '排序',
    create_time int unsigned not null default 0,
    update_time int unsigned null default 0,

    key order_id (order_id),
    PRIMARY key (id)
)ENGINE=INNODB  DEFAULT CHARSET=utf8


// 会员 member
create table member(
    id int unsigned not null auto_increment,
    username varchar(32) not null default '' comment '用户名',
    password varchar(32) not null default '' comment '密码',
 
    level_id int unsigned not null default '' comment '层级id',
    sc_id int unsigned not null default '' comment '股东id',

    app_id varchar(32) not null default '' comment '设备id', 
    
    clock_in_score int unsigned not null default 0 comment '签到积分',

     
    state TINYINT not null default 0 comment '状态 0关闭 1开启',
    order_id int not null default 0 comment '排序',
    create_time int unsigned not null default 0,
    update_time int unsigned not null default 0,

    key username (username),
    key level_id (level_id),
    key sc_id (sc_id),
    key app_id (app_id),
    PRIMARY key (id)
)ENGINE=INNODB  DEFAULT CHARSET=utf8


// 签到记录  clock_in_log
create table clock_in_log(
    id int unsigned not null auto_increment,

    member_id int unsigned not null default 0 comment '会员id',
date int unsigned not null default 0 comment '签到日期 Ymd',

    create_time int unsigned not null default 0,
    
    key member_id(member_id),
    key date(date),
    key create_time(create_time),
    PRIMARY key (id)
)ENGINE=INNODB  DEFAULT CHARSET=utf8


