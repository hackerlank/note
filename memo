//支付类型
pay_type

create table vc_pay_type(
    id SMALLINT UNSIGNED AUTO_INCREMENT,
    name char(32) not null default '',
    nick char(32) not null default '' comment '中文名称',
    state TINYINT(1) UNSIGNED not null default 0 comment '状态 0关闭 1开启 2维护',
    logo char(32) not null default '' comment '图片地址',
    default_opt MEDIUMINT unsigned not null default 0 comment '默认支付方式',
    is_mobile tinyint unsigned not null default 0 comment '是否为移动端支付类型  0 不是  1 是',
    sort int unsigned not null default 0,
    PRIMARY KEY (`id`)  
)ENGINE=InnoDB DEFAULT CHARSET=utf8 comment '支付类型';


//支付商家
pay_merchant

create table vc_pay_merchant(
    id MEDIUMINT UNSIGNED AUTO_INCREMENT,
    name char(32) not null default '',
    path char(16) not null default '' comment '文件夹名', 
    state TINYINT(1) UNSIGNED not null default 0 comment '状态 0关闭 1开启 2维护',
    sort int unsigned not null default 0,
    create_time int unsigned not null default 0,
    update_time int unsigned not null default 0,
    PRIMARY KEY (`id`)  
)ENGINE=InnoDB DEFAULT CHARSET=utf8 comment '支付商家';


//支付方式
vc_pay_type_merchant

create table vc_pay_type_merchant(
    id MEDIUMINT UNSIGNED AUTO_INCREMENT,
    type_id SMALLINT UNSIGNED not null default 0 comment '支付类型id',
    merchant_id MEDIUMINT UNSIGNED not null default 0 comment '支付商家id',
    code char(32) not null default '' comment '支付code',
    min_amount int UNSIGNED not null default 0 comment '最小限额',
    max_amount int UNSIGNED not null default 0 comment '最大限额',
    state TINYINT(1) UNSIGNED not null default 0 comment '状态 0关闭 1开启 2维护',
    action char(16) not null default '' comment '执行的方法名',
    extend_name char(32) not null default '' comment '支付扩展名',
    sort int unsigned not null default 0,
    create_time int unsigned not null default 0,
    update_time int unsigned not null default 0,
    PRIMARY KEY (`id`)  
)ENGINE=InnoDB DEFAULT CHARSET=utf8 comment '支付方式';





