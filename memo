create table vc_rqcode(
    id int unsigned not null auto_increment,
    username varchar(32) not null default '' comment '账号',
    image_url varchar(64) not null default '' comment '二维码地址',
    type tinyint UNSIGNED not null default 0 comment '类型',
  state TINYINT not null default 0 comment '状态 0关闭 1开启',
    order_id int not null default 0 comment '排序',
  create_time datetime not null default '0000-00-00 00:00:00',
  update_time datetime not null default '0000-00-00 00:00:00',
    PRIMARY key (id)
)ENGINE=INNODB  DEFAULT CHARSET=utf8

